/* pages/start/createNew.wxss */
page{
  background: #121A21;
}
.box{
  padding: 20rpx 60rpx 140rpx 60rpx;
}
.pop{
  width: 120rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: black;
  background: #D0A258;
  border-radius: 10rpx;
  text-align: center;
}
.u-tri{
  content: '';
  height: 0rpx;
  width: 0rpx;
  border: 10rpx solid;
  border-color: transparent #D0A258 transparent transparent;
}
.text{
  color: rgba(208, 162, 88, 1);
  font-size: 30rpx;
}
.star::after{
  content: '*';
  color: red;
}
.input{
  width: 630rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 6rpx;
  background-color: rgba(28, 48, 67, 1);
  border: 1rpx solid rgba(70, 96, 125, 1);
  padding-left: 20rpx;
  margin-top: 10rpx;
  box-sizing: border-box;
  color: #aaa;
}
.tips{
  color: #46607D;
  font-size: 20rpx;
  width: 630rpx;
  display: inline-block;
  margin-top: 10rpx;
}
.add-pic{
  width: 120rpx;
  height: 120rpx;
  line-height: 120rpx;
  border-radius: 6rpx;
  background-color: rgba(28, 48, 67, 1);
  text-align: center;
  border: 2rpx solid rgba(70, 96, 125, 1);
  color: #46607D;
  font-size: 50rpx;
  margin-top: 20rpx;
  /* margin-left: 20rpx; */
  /* display: inline-block; */
}
.del-pic{
  color: #D0A258;
}
