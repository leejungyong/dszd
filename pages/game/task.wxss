.plant{
  width: 660rpx;
  height: 900rpx;
  border-radius:10rpx;
  background-color: rgba(40, 56, 74, 0.93);
  border: 4rpx solid var(--yellow);
  margin: 60rpx auto 0 auto;
  padding: 50rpx;
  box-sizing: border-box;
  color: var(--yellow)
}
.plant .ch{
  text-align: left;
  margin-top: 30rpx;
}
.plant .radio-group{
  text-align: left;
  margin-top: 30rpx;
}
.plant .radio-group .radio{
  width: 120rpx;
}
.plant .money{
  margin-top: 20rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx dashed var(--word-color);
  border-bottom: 1rpx dashed var(--word-color);
}
.plant .total{
  font-size: 30rpx;
  margin: 30rpx 0 50rpx 0;
  text-align: left;
}
.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(18, 26, 33, 0.8);
  text-align: center;
  font-family: -apple-system;
}
.close{
    width: 90rpx;
    height: 90rpx;
    line-height: 90rpx;
    border-radius: 50%;
    background-color: rgba(18, 26, 33, 1);
    border: 2rpx solid rgba(208, 162, 88, 1);
    color: #d0a258;
    font-size: 50rpx;
    position: absolute;
    right: 10rpx;
    top: 10rpx;
}

